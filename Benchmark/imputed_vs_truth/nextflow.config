params {
    imputed_files = "/path/to/dosage/*.vcf"
    truth_files= "/path/to/truth/.*.vcf"
}


process {
        executor = "slurm"
        clusterOptions = "--account=rrg-vmooser"
        cpus = 1
        time = "2d"
        memory = "16G"
}

executor {
    $slurm {
        queueSize = 1000
        jobName = { "postimputation" }
}
}
