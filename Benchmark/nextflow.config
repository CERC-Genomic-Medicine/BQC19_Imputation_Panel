params {
  imputed_files = "path/to/imputed_vcf/files/*.vcf"
  truth_files_first_reference = "path/to/first_reference/truth/files/*.vcf"
  truth_files_second_reference = "path/to/second_reference/truth/files/*.vcf"
  



  chromosomes = ["chr1", "chr2", "chr3", "chr4", "chr5", "chr6", "chr7", "chr8", "chr9", "chr10", "chr11", "chr12", "chr13", "chr14", "chr15", "chr16", "chr17", "chr18", "chr19", "chr20", "chr21", "chr22"]
  
}


process {
        executor = "slurm"
        clusterOptions = "--account="
        cpus = 1
        time = "2d"
        memory = "16G"
}

executor {
    $slurm {
        queueSize = 1000
        jobName = { "postimputation" }
}
}